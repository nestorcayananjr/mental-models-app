generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Confidence {
  ONE
  TWO
  THREE
  FOUR
  FIVE
}

model Pattern {
  id          String @id @default(uuid())
  name        String
  category    String
  trigger     String
  problem     String
  solution    String
  examples    String[]
  confidence  Confidence
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}